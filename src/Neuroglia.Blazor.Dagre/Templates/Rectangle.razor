<rect 
    x="@Position.X" 
    y="@Position.Y" 
    width="@Size.Width" 
    height="@Size.Height"
    rx="@(RadiusX ?? 0)"
    ry="@(RadiusY ?? 0)"
/>

@code {
    
    [Parameter] public virtual Point Position { get; set; } = new(0, 0);

    [Parameter] public virtual Size Size { get; set; } = new(100, 30);

    [Parameter] public virtual double? RadiusX { get; set; }
    
    [Parameter] public virtual double? RadiusY { get; set; }

}
