<rect 
    x="@(BBox.X.ToInvariantString())" 
    y="@(BBox.Y.ToInvariantString())" 
    width="@(BBox.Width.ToInvariantString())" 
    height="@(BBox.Height.ToInvariantString())"
    rx="@((RadiusX ?? 0).ToInvariantString())"
    ry="@((RadiusY ?? 0).ToInvariantString())"
/>

@code {
    
    [Parameter] public virtual IBoundingBox BBox { get; set; } = null!;
    
    [Parameter] public virtual double? RadiusX { get; set; }
    
    [Parameter] public virtual double? RadiusY { get; set; }

}
