<rect 
    x="@(Bounds.X.ToInvariantString())" 
    y="@(Bounds.Y.ToInvariantString())" 
    width="@(Bounds.Width.ToInvariantString())" 
    height="@(Bounds.Height.ToInvariantString())"
    rx="@((RadiusX ?? 0).ToInvariantString())"
    ry="@((RadiusY ?? 0).ToInvariantString())"
/>

@code {
    
    [Parameter] public virtual BoundingBox Bounds { get; set; } = null!;
    
    [Parameter] public virtual double? RadiusX { get; set; }
    
    [Parameter] public virtual double? RadiusY { get; set; }

}
