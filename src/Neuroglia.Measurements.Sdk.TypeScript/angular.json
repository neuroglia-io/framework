{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "@neuroglia/measurements": {
      "projectType": "library",
      "root": "packages/neuroglia/posm-project-manager-client",
      "sourceRoot": "packages/neuroglia/posm-project-manager-client/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/neuroglia/posm-project-manager-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/neuroglia/posm-project-manager-client/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/neuroglia/posm-project-manager-client/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "packages/neuroglia/posm-project-manager-client/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "packages/neuroglia/posm-project-manager-client/**/*.ts",
              "packages/neuroglia/posm-project-manager-client/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
